{
  "name": "irl",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clearPort": "npx kill-port 8080 & npx kill-port 8081",
    "dev": "npm run clearPort & concurrently npm:serve npm:listen:tailwind npm:server",
    "serve": "vue-cli-service serve --port 8080",
    "server": "json-server --watch db/index.js --routes db/routes.json --port 8081",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "cypress open --project ./config",
    "lint": "vue-cli-service lint",
    "listen:tailwind": "npx tailwindcss -i ./src/styles/tailwind.index.css -o ./src/styles/tailwind.output.css --watch"
  },
  "dependencies": {
    "@kong/kongponents": "7.0.0-beta.24",
    "axios": "0.27.2",
    "core-js": "3.22.4",
    "gsap": "3.10.4",
    "register-service-worker": "1.7.2",
    "vue": "3.2.33",
    "vue-class-component": "7.2.6",
    "vue-router": "4.0.15",
    "vuex": "4.0.2"
  },
  "devDependencies": {
    "@faker-js/faker": "6.3.1",
    "@types/jest": "27.5.0",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "@vue/cli-plugin-babel": "5.0.4",
    "@vue/cli-plugin-e2e-cypress": "5.0.4",
    "@vue/cli-plugin-eslint": "5.0.4",
    "@vue/cli-plugin-pwa": "5.0.4",
    "@vue/cli-plugin-router": "5.0.4",
    "@vue/cli-plugin-typescript": "5.0.4",
    "@vue/cli-plugin-unit-jest": "5.0.4",
    "@vue/cli-plugin-vuex": "5.0.4",
    "@vue/cli-service": "5.0.4",
    "@vue/eslint-config-typescript": "10.0.0",
    "@vue/test-utils": "2.0.0-rc.21",
    "@vue/vue3-jest": "27.0.0",
    "autoprefixer": "10.4.7",
    "babel-jest": "28.1.0",
    "concurrently": "7.1.0",
    "cypress": "9.6.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-vue": "8.7.1",
    "jest": "28.1.0",
    "json-server": "0.17.0",
    "postcss": "8.4.13",
    "prettier": "2.6.2",
    "sass": "1.51.0",
    "sass-loader": "12.6.0",
    "tailwindcss": "3.0.24",
    "ts-jest": "28.0.1",
    "typescript": "4.6.4"
  }
}
